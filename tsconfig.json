{
    "compilerOptions": {
      "outDir": "./dist",
      "sourceMap": true,
      "strict": true,
      "target": "ES6",
      "module": "ES6",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*.ts"]
  }
  